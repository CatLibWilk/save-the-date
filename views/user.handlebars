<h2>Your Profile</h2>
<h3>{{user.firstname}} {{user.lastname}}</h3>

<h4>Profile information</h4>
    <h5>{{user.email}}</h5>
    <h5>{{user.phone}}</h5>
    <h5>Member since: {{user.createdAt}}</h5>


<h4>Message History</h4>
    {{#each user.Messages}}
        <div class="bg-secondary m-3">
            <h5>{{this.body}}<h5>
            <h5>sent on: {{this.sendTime}}</h5>
            <div class="btn btn-warning" data-id={{this.id}}>Resend</div>
        </div>
    {{/each }}
